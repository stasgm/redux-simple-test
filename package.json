{
  "name": "react-redux-typescript",
  "version": "0.0.5",
  "description": "Simple react-redux-typescript CRUD example",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=configs/tsconfig.webpack.json webpack-dev-server --config configs/webpack/webpack.config.dev.ts",
    "build": "cross-env TS_NODE_PROJECT=configs/tsconfig.webpack.json webpack --progress --display-modules --config configs/webpack/webpack.config.prod.ts",
    "test": "tsc && jest",
    "clean": "rimraf dist",
    "fix:ts": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "prettier \"**/*.@(ts|tsx|js|jsx)\"",
    "prettier:fix": "npm run prettier:cli -- --write",
    "prettier:lint": "npm run prettier:cli -- -l",
    "reinstall": "bash -c 'rm -rf  package-lock.json node_modules' && npm i",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "tslint:cli": "tslint --project tsconfig.json",
    "tslint:fix": "npm run tslint:cli -- --fix",
    "tslint:lint": "npm run tslint:cli"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
